{
	"manifest_version": 3,
	"name": "Bookmark Manager Plus M3",
	"version": "1.0.0.0",
	"description": "Search bookmarks and do whatever you can imagine.",
	"options_ui": {
		"page": "options.html",
		"open_in_tab": true
	},
	"icons": {
		"16": "icon/icon16.png",
		"32": "icon/icon32.png",
		"48": "icon/icon48.png",
		"128": "icon/icon128.png"
	},
	"content_security_policy": {
		"extension_pages": "script-src 'self'; object-src 'self'; style-src 'self' 'unsafe-inline' https://code.jquery.com https://maxcdn.bootstrapcdn.com"
	},
	"action": {
		"default_icon": {
			"16": "icon/icon16.png",
			"32": "icon/icon32.png"
		},
		"default_popup": "popup.html"
	},	
	"background": {
		"service_worker": "js/background.js"
	},
	"permissions": [
		"bookmarks",
		"tabs",
		"storage",
		"favicon"
	]
}